#:import Factory kivy.factory.Factory



<Label>:
    markup: True
    color: 0,0,0,1  # This defaults to 1,1,1,1 when not used

<TextInput>:
    write_tab: False

<Button1@Button>:
    background_normal: ''
    background_color: 0, 0, 0, 0
    halign: 'center'
    valign: 'bottom'
    canvas.before:
        Color:
            rgba: (.55, .55, .55, 1) if self.state == 'normal' else (0, .7, .7, 1)
        RoundedRectangle:
            pos: self.pos
            size: self.size
            radius: [20,]





<BackgroundColor@Widget>:
    background_color: 0.7, 0, 0, 0.5

<PopUp>
    markup: True
    color: 0.5,0.5,0.5,1
    background_color: 100, 111, 111, 0.7

<FinestraLogin>:
    name: FinestraLogin
    id : FinestraLogin
    button11 : button11
    GridLayout:

        cols: 1
        rows: 3
        TextInput:
            id: inputnomepg
            font_size: 32
			multiline: False
            text: ""
        Button1:
            background_color: 1, 1, 1, 1
            size_hint: 0.3, 0.3
            text: "select Principale"
            on_release: inputnomepg.text = "Principale"
        Button1:
            background_color: 1, 1, 1, 1
            size_hint: 0.3, 0.3
            id: button11
            text: "LOGIN"
            on_release: app.FPBtn(inputnomepg.text)


<FinestraTransizione>:
    name : FinestraTransizione
    id: FinestraTransizione


<FinestraPrincipale>:
    canvas.before:
        Color:
            rgba: 1, 1, 1, 1
        Rectangle:
            pos: self.pos
            size: self.size
